     "name": "stdout",
     "output_type": "stream",
     "text": [
      "34.1 ms ± 12.3 ms per loop (mean ± std. dev. of 7 runs, 100 loops each)\n"
     ]
    }
   ],
   "source": [
    "%%timeit\n",
    "df=generate_df(10000)\n",
    "df['reward']=df['hate_food']\n",
    "df.loc[((df['pct_sleeping']>0.5) & \n",
    "        (df['time_in_bed'] >5)|\n",
    "        df['age']<90),'reward']=df['favorite_food']"
   ]
